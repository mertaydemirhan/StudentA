
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Studenta Admin</title>

    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="~/Content/AdminLTE-3.0.4/plugins/fontawesome-free/css/all.min.css" rel="stylesheet" />


 
     <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/AdminLTE-3.0.4/dist/css/adminlte.min.css">
  
 
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">


</head>
<body class="hold-transition sidebar-mini layout-fixed sidebar-collapse">
    <div class="wrapper">

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>

            </ul>
        </nav>

        <aside class="main-sidebar sidebar-light-primary elevation-4">


            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/Content/Registration-form/img/logo.png" width="100%" height="100" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Studenta Admin</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item has-treeview" style="margin-left:2px">
                            <a href="/Panel/Dashboard" class="nav-link">
                                <i class="nav-icon fas fa-home"></i>
                                <p>
                                    Dashboard
                                </p>
                            </a>
                        </li>

                        <li class="nav-item has-treeview menu-open">
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Students/Index" class="nav-link">
                                        <i class="nav-icon fas fa-user-graduate"></i>
                                        <p>
                                            Students
                                        </p>
                                    </a>
                                </li>


                                <li class="nav-item">
                                    <a href="/Roles/Index" class="nav-link">
                                        <i class="nav-icon fa fa-tasks"></i>
                                        <p>
                                            User Roles
                                        </p>
                                    </a>
                                </li>

                                <li class="nav-item">
                                    <a href="/Settings/Index" class="nav-link">
                                        <i class="nav-icon fas fa-cog"></i>
                                        <p>
                                            Settings
                                        </p>
                                    </a>
                                </li>



                                <li class="nav-item">
                                    <a href="#" id="myButton" class="nav-link">
                                        <i class="nav-icon fas fa-sign-out-alt"></i>
                                        <p>
                                            Log Out
                                        </p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>

                </nav>
            </div>
        </aside>

        <div class="content-wrapper">
            <div class="content-header">

            </div>
            <section class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </section>
        </div>

    </div>

    <script src="~/Content/AdminLTE-3.0.4/dist/js/adminlte.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="~/Content/AdminLTE-3.0.4/dist/js/pages/dashboard.js"></script>
    <script src="~/Content/AdminLTE-3.0.4/plugins/sparklines/sparkline.js"></script>
    <script src="~/Content/AdminLTE-3.0.4/plugins/jquery-knob/jquery.knob.min.js"></script>
    <script src="~/Content/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/AdminLTE-3.0.4/dist/js/adminlte.js"></script>

    <!-- AdminLTE for demo purposes -->


    <script type="text/javascript">
        document.getElementById("myButton").onclick = function () {
            swal({
                title: "Are you sure?",
                text: "Are you sure you want to log out?",
                icon: "warning",
                buttons: true,
                dangerMode: true,
                closeOnClickOutside: false,
            })
                .then((willLogOut) => {
                    if (willLogOut) {
                        location.href = "/Admin/Logout";
                    }
                });
        };
    </script>


</body>
</html>