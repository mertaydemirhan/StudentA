
@{
    Layout = null;
}
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Profile  </title>
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->

    <link href="~/Content/UserDashboard/plugins/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet" href="~/Content/UserDashboard/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/Content/UserDashboard/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="~/Content/UserDashboard/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/UserDashboard/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/Content/UserDashboard/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/Content/UserDashboard/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="~/Content/UserDashboard/plugins/summernote/summernote-bs4.min.css">
    <style>
        .brand-imagehakan {
            line-height: .8;
            margin-left: 1.1rem;
            margin-right: 0.5rem;
            margin-top: -3px;
            max-height: 45px;
            width: auto;
        }

        .wrapper {
            min-height: 90%;
        }

        .sidep {
            font-weight: 500;
            font-family: "Roboto", "Helvetica", "Arial", sans-serif;
            line-height: 1.5em;
            font-size: 14px;
            color: rgb(20, 20, 20);
        }
    </style>
</head>

<body class="hold-transition sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <!-- Navbar -->
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-light-primary elevation-4">
            <!-- Brand Logo -->
            <a href="" class="brand-link">
                <img src="~/Content/UserDashboard/img/logo.png" alt=" Logo" class="brand-imagehakan ">

            </a>
            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">

                    <div class="info">
                        <a href="/Welcome/Welcome" class="d-block"
                           style="font-family:sans-serif; font-weight: bold; margin-left: 20px;">Profile</a>
                    </div>

                </div>
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu"
                        data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->

                        <li class="nav-item has-treeview">
                            <a href="" class="nav-link">
                                <i class="nav-icon fas fa-user"></i>
                                <p class="sidep">
                                    User Info
                                </p>
                            </a>

                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("ApplicationIndex","Application")" class="nav-link">
                                <i class="nav-icon fas fa-th"></i>
                                <p class="sidep">
                                    Applications Info
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="" class="nav-link">
                                <i class="nav-icon far fa-envelope"></i>
                                <p class="sidep">
                                    Contact
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/User/Login/" class="nav-link">
                                <i class="nav-icon far fa-flag"></i>
                                <p class="sidep">
                                    Log out
                                </p>
                            </a>
                        </li>

                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content">
                <div class="row">
                    <div class="col-md-12">
                        @RenderBody()

                    </div>
                </div>

            </section>
            <!-- /.content -->
        </div>
    </div>
    <!-- jQuery -->
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Content/UserDashboard/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <!-- Bootstrap 4 -->
    <script src="~/Content/UserDashboard/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="~/Content/UserDashboard/plugins/chart.js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Content/UserDashboard/plugins/sparklines/sparkline.js"></script>
    <!-- JQVMap -->
    <script src="~/Content/UserDashboard/plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/Content/UserDashboard/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Content/UserDashboard/plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/Content/UserDashboard/plugins/moment/moment.min.js"></script>
    <script src="~/Content/UserDashboard/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/Content/UserDashboard/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="~/Content/UserDashboard/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/Content/UserDashboard/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/UserDashboard/dist/js/adminlte.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/Content/UserDashboard/dist/js/pages/dashboard.js"></script>
</body>

</html>